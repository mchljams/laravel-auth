{
    "name": "mchljams/laravel-auth",
    "description": "Laravel Auth Backend.",
    "keywords": ["laravel", "auth", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael James",
            "email": "mchljams@lmchljams.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mchljams\\LaravelAuth\\": "src/",
            "Illuminate\\Foundation\\Auth\\": "auth/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Mchljams\\LaravelAuth\\LaravelAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
